{
  "name": "vuex-c3s",
  "version": "2.2.9-beta",
  "description": "SDK for the Citizen Science Framework API, using Swagger and Vuex (requires JTOS >= 0.8.0 to be running on your server side)",
  "directories": {
    "test": "test"
  },
  "main": "dist/vuex-c3s.umd.js",
  "module": "dist/vuex-c3s.es.js",
  "files": [
    "dist",
    "test"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "minify": "uglifyjs dist/vuex-c3s.umd.js --output dist/vuex-c3s.min.js --compress --mangle",
    "test": "jest",
    "docs": "jsdoc -c jsdoc.conf.json src/ ./DOCS.md",
    "md-docs": "jsdoc2md \"src/**/*.js\" > DOCS.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citizensciencecenter/vuex-c3s.git"
  },
  "keywords": [
    "vuex",
    "vue",
    "i18n",
    "c3s",
    "swagger",
    "citizen-science"
  ],
  "author": {
    "name": "Chris Gwilliams",
    "email": "encima@gmail.com",
    "url": "https://github.com/encima"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/citizensciencecenter/vuex-c3s/issues"
  },
  "homepage": "https://github.com/citizensciencecenter/vuex-c3s#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "minio": "^7.0.12",
    "rison-node": "^2.1.0",
    "swagger-client": "^3.9.4",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "babel-jest": "^24.9.0",
    "docdash": "^1.1.1",
    "eslint": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-html": "^6.0.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "jsdoc-export-default-interop": "^0.3.1",
    "jsdoc-to-markdown": "^5.0.3",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "testTimeout": 10000
  }
}
