{
    "tags": {
        "allowUnknownTags": true
    },
    "opts": {
      "destination":"./docs/",
      "recurse": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "plugins": [
      "plugins/summarize"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
